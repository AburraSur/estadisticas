<!DOCTYPE html>
<html>
    <head>
        {% block title %}
        {% endblock title %}
       {% block stylesheets %}
            {% stylesheets 'bundles/app/css/*' filter='cssrewrite' %}
                <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
    </head>
    <body>
        {% block content %}
        {% endblock content %}
    </body>
    {% block javascripts %}
            {% javascripts 
                '@jquery_and_ui'
                '@AppBundle/Resources/public/js/*' %}
                <script src="{{ asset_url }}"></script>
            {% endjavascripts %}
            
            <script>
                $(document).ready(function() {
                    $('.datepicker').datepicker({
                        format: 'yyyy-mm-dd'
                    });
                });
            </script>
        {% endblock %} 
</html>
